{
  "name": "jumpstart",
  "version": "0.0.1",
  "description": "ES6 module starter kit with Webpack, Babel, Mocha, Chai, Sinon, and Istanbul",
  "repository": {
    "type": "git",
    "url": "https://github.com/ctaylo21/JumpStart.git"
  },
  "keywords": [
    "es6",
    "javascript",
    "mocha",
    "chai",
    "sinon",
    "webpack",
    "babel",
    "istanbul"
  ],
  "author": "Caleb Taylor <caleb89taylor@gmail.com",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --open --config webpack/dev.js",
    "build": "webpack -p --config webpack/prod.js",
    "test": "NODE_ENV=test ./node_modules/nyc/bin/nyc.js mocha-webpack",
    "tdd": "mocha-webpack --watch",
    "tdd:web": "webpack-dev-server --inline --progress --open --config webpack/webTest.js"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "exclude": [
      ".tmp",
      "test"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.17",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "html-webpack-plugin": "^2.30.1",
    "jsdom": "11.3.0",
    "jsdom-global": "3.0.2",
    "mocha": "^4.1.0",
    "mocha-webpack": "^1.0.1",
    "nyc": "^11.4.1",
    "path": "^0.12.7",
    "sinon": "^4.1.6",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {}
}
